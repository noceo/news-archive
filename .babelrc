{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    [
      "module-resolver",
      {
        "alias": {
          "@": "./src",
          "@helpers": "./src/helpers",
          "@jobs": "./src/jobs",
          "@middleware": "./src/middleware",
          "@services": "./src/services"
        }
      }
    ]
  ]
}
